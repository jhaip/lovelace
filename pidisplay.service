[Unit]
Description=Programmable Space Raspberry Pi
After=network.target

[Service]
Type=oneshot
User=root
# Copy this file to /etc/systemd/system
# sudo systemctl daemon-reload
# sudo systemctl start progspace
# Run at boot: sudo systemctl enable progspace
Environment=PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/local/games:/usr/games
RemainAfterExit=yes
ExecStart=/home/jacob/lovelace/jig pieditorstart
ExecStop=/home/jacob/lovelace/jig stop

[Install]
WantedBy=multi-user.target